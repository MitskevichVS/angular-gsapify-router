{
  "extends": "airbnb-base/legacy",
  "rules": {
    "comma-dangle": [1, "always-multiline"],
    "quotes": [2, "single"],
    "prefer-const": 0,
    "no-shadow": 0,
    "no-param-reassign": 0,
    "no-var": 0,
    "vars-on-top": 0,
    "func-names": 0,
    "max-len": 1,
    "no-unused-vars": 1
  }
}
